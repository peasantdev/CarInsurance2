@model IEnumerable<CarInsurance2.Models.Insuree>

@{
    ViewBag.Title = "Admin";
}

<h2>Admin</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<h1>Admin View - All Quotes</h1>

<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Quote</th>
    </tr>
    @foreach (var quote in Model)
    {
        <tr>
            <td>@quote.FirstName</td>
            <td>@quote.LastName</td>
            <td>@quote.EmailAddress</td>
            <td>@quote.Quote</td>
        </tr>
    }
</table>


